{
  "name": "generator-react-client",
  "version": "1.0.0",
  "description": "A  SPA generator based on React + Redux + Styled Components + Jest + React Test Library",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development webpack serve --watch --color --progress",
    "test": "NODE_ENV=test jest --config jest.config.json --passWithNoTests",
    "test:snap": "npm run test -- -u",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "test:ci": "npm test -- --coverage --runInBand",
    "lint:code:check": "eslint './src/**/*.{js,jsx,spec.js}'",
    "lint:code:write": "npm run lint:check -- --fix",
    "lint:style:check": "stylelint ./src/**/*.{js,jsx} --aei",
    "lint:style:write": "npm run lint:style:check -- --fix",
    "lint": "npm run lint:code:check && npm run lint:style:check",
    "build:destroy": "rm -rf dist",
    "build:clean": "npm run build:destroy && mkdir dist",
    "build:move": "cp -r ./public ./dist && mv ./dist/assets/index.html ./dist",
    "build": "npm run build:clean && webpack && npm run build:move"
  },
  "keywords": [
    "React",
    "Redux",
    "Styled",
    "Components",
    "Jest",
    "React",
    "Test",
    "Library"
  ],
  "author": "Wallace Gleen <wallace.gleen@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/eslint-plugin": "^7.12.1",
    "@babel/node": "^7.12.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.3.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-rewire-exports": "^2.2.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "jest-prop-type-error": "^1.1.0",
    "prettier": "^2.2.1",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^3.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.4.0"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0"
  }
}
